

class Multiplikator():
    class KlammerLinksAddition():
        class VariableLinks(self, var1):
            def __init__(self, var1):
                self.var1 = var1
        class VariableRechts(self, var2):
            def __init__(self, var2):
                self.var2 = var2
        varKL = VariableLinks.var1 + VariableRechts.var2
        print(varKL)
    class KlammerRechtsMinus:
        class VariableLinks(self, var3):
            def __init__(self, var3):
                self.var3 = var3
        class VariableRechts(self, var4):
            def __init__(self, var4):
                self.var4 = var4

